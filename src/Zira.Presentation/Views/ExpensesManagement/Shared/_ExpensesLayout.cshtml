@await Html.PartialAsync("Shared/_Styles")

<body>
<!-- Layout wrapper -->
<div class="layout-wrapper layout-content-navbar">
    <div class="layout-container">
        <!-- Menu -->
        @await Html.PartialAsync("Shared/_Sidebar")
        <!-- / Menu -->

        <!-- Layout container -->
        <div class="layout-page">
            <!-- Navbar -->
            @await Html.PartialAsync("Shared/_TopBar")
            <!-- / Navbar -->

            <!-- Content -->
            @RenderBody()
            <!-- / Content -->

            <!-- Footer -->
            @await Html.PartialAsync("Shared/_Footer")
            <!-- / Footer -->

            <div class="content-backdrop fade"></div>
        </div>
        <!-- Content wrapper -->
    </div>
    <!-- / Layout page -->
</div>
<!-- / Layout wrapper -->
</body>

@await Html.PartialAsync("Shared/_Scripts")