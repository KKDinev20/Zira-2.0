@{
    Layout = "Shared/_DashboardLayout";
}
<!DOCTYPE html>

<html
    lang="en"
    class="light-style layout-menu-fixed"
    dir="ltr"
    data-theme="theme-default"
    data-assets-path="../assets/"
    data-template="vertical-menu-template-free">
<body>
<!-- Layout wrapper -->
<div class="layout-wrapper layout-content-navbar">
<div class="layout-container">
<!-- Menu -->
@await Html.PartialAsync("Shared/_Sidebar")
<!-- / Menu -->

<!-- Layout container -->
<div class="layout-page">
<!-- Navbar -->

@await Html.PartialAsync("Shared/_TopBar")

    
<div class="content-wrapper">

<div class="container-xxl flex-grow-1 container-p-y">
<div class="row">
@await Html.PartialAsync("Shared/_WelcomeBack")
@await Html.PartialAsync("Shared/_RevenueCards")
@await Html.PartialAsync("Shared/_TotalRevenueChart")
<!-- Total Revenue -->
@await Html.PartialAsync("Shared/_PaymentTransactionCards")
<!--/ Total Revenue -->
</div>
<div class="row">
<!-- Order Statistics -->
<div class="col-md-6 col-lg-4 col-xl-4 order-0 mb-4">
    @await Html.PartialAsync("Shared/_ExpenseCategories")
</div>
<!--/ Order Statistics -->

<!-- Expense Overview -->
<div class="col-md-6 col-lg-4 order-1 mb-4">    
    @await Html.PartialAsync("Shared/_ExpenseOverview")
</div>
<!--/ Expense Overview -->

<!-- Transactions -->
<div class="col-md-6 col-lg-4 order-2 mb-4">
    @await Html.PartialAsync("Shared/_RecentTransactions")

</div>
<!--/ Transactions -->
</div>
</div>
<!-- / Content -->

<!-- Footer -->
@await Html.PartialAsync("Shared/_Footer")

<!-- / Footer -->

<div class="content-backdrop fade"></div>
</div>
<!-- Content wrapper -->
</div>
<!-- / Layout page -->
</div>

</div>
</body>
</html>