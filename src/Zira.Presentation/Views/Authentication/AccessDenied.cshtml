@{
    ViewData["Title"] = "Access Denied";
    Layout = "Shared/_AuthenticationLayout";
}

<body>
<div class="container-xxl container-p-y">
    <div class="misc-wrapper">
        <h2 class="mb-2 mx-2">@AuthenticationText.PageNotFoundTitle</h2>
        <p class="mb-4 mx-2">@AuthenticationText.PageNotFoundMessage</p>
        <a asp-controller="Home" asp-action="Index" class="btn btn-primary">@AuthenticationText.BackToLogin</a>
        <div class="mt-3">
            <img
                src="/dashboard/assets/img/illustrations/page-misc-error-light.png"
                alt="page-misc-error-light"
                width="500"
                class="img-fluid"
                data-app-dark-img="illustrations/page-misc-error-dark.png"
                data-app-light-img="illustrations/page-misc-error-light.png"/>
        </div>
    </div>
</div>
</body>